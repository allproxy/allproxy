{
  "name": "allproxy",
  "version": "2.5.43",
  "description": "AllProxy: HTTP, SQL, gRPC Debugging Tool.",
  "keywords": [
    "proxy",
    "debug",
    "mitm",
    "http",
    "https",
    "http2",
    "sql",
    "grpc",
    "tcp"
  ],
  "homepage": "https://github.com/davechri/allproxy",
  "bugs": {
    "url": "https://github.com/davechri/allproxy/issues",
    "email": "davechri58@gmail.com"
  },
  "main": "app.js",
  "bin": {
    "allproxy": "bin/allproxy",
    "allproxy-ca": "bin/allproxy-ca",
    "allproxhy-system-proxy": "bin/allproxy-system-proxy"
  },
  "scripts": {
    "install": "cd ./node-http-mitm-proxy && $npm_execpath install && cd .. && npm run install-client",
    "install-client": "[ -d ./client/src ] && cd ./client && $npm_execpath install; exit 0",
    "postinstall": "[ ! -d ./build ] && npm run build || npm run symbolic-links",
    "startdev": "NODE_ENV=development nodemon --config nodemon.json app.ts && tsc --force --project ./tsconfig.json",
    "build": "npm run build-server && npm run build-client",
    "build-server": "npm run clean && tsc --project ./tsconfig.json && npm run symbolic-links",
    "build-client": "npm run clean-client && cd ./client && npm run build",
    "clean": "rm -rf ./build",
    "clean-client": "rm -rf ./client/build",
    "start": "if ./scripts/noDir.sh ./build; then npm run build; fi; if ./scripts/noDir.sh ./client/build; then npm run build-client; fi; NODE_ENV=production node ./build/app.js",
    "symbolic-links": "[ -d ./build ] && ln -sf ../node-http-mitm-proxy/ ./build/",
    "startforever": "./scripts/startForever.sh",
    "stopforever": "./scripts/stopForever.sh",
    "test": "cd ./client && npm run test",
    "help": "if ./scripts/noDir.sh ./build; then npm run build; fi; node ./build/app.js --help; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davechri/allproxy.git"
  },
  "author": "Dave Christenson",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "bufferutil": "^4.0.3",
    "decompress-response": "^3.3.0",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "net": "^1.0.2",
    "node-fetch": "^2.6.7",
    "path": "^0.12.7",
    "protobufjs": "^6.11.2",
    "socket.io": "^4.0.1",
    "sql-formatter": "^2.3.3",
    "tls": "^0.0.1",
    "url": "^0.11.0",
    "utf-8-validate": "^5.0.4"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.5",
    "@types/node": "^14.14.35",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "@types/sql-formatter": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
