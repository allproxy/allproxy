{
  "name": "allproxy",
  "version": "3.4.0",
  "description": "AllProxy: MITM HTTP Debugging Tool.",
  "keywords": [
    "proxy",
    "debug",
    "mitm",
    "fiddler",
    "charles",
    "http",
    "https",
    "sql",
    "grpc",
    "tcp"
  ],
  "homepage": "https://github.com/davechri/allproxy",
  "bugs": {
    "url": "https://github.com/davechri/allproxy/issues",
    "email": "davechri58@gmail.com"
  },
  "main": "main.js",
  "bin": {
    "allproxy": "bin/allproxy",
    "allproxy-ca": "bin/allproxy-ca",
    "allproxy-system-proxy": "bin/allproxy-system-proxy"
  },
  "scripts": {
    "install": "cd ./node-http-mitm-proxy && yarpm install && cd .. && npm run install-client",
    "install-client": "[ -d ./client/src ] && cd ./client && yarpm install; exit 0",
    "postinstall": "[ ! -d ./build ] && npm run build || npm run symbolic-links",
    "startdev": "NODE_ENV=development nodemon --config nodemon.json app.ts && tsc --force --project ./tsconfig.json",
    "build": "npm run build-server && npm run build-client",
    "build-server": "npm run clean && tsc --project ./tsconfig.json && npm run symbolic-links",
    "build-client": "npm run clean-client && cd ./client && npm run build",
    "clean": "rimraf ./build",
    "clean-client": "rimraf ./client/build",
    "start": "electron-forge start",
    "start-headless": "if ./scripts/noDir.sh ./build; then npm run build; fi; if ./scripts/noDir.sh ./client/build; then npm run build-client; fi; NODE_ENV=production node ./build/app.js",
    "symbolic-links": "[ -d ./build ] && ln -sf ../node-http-mitm-proxy/ ./build/ && ln -sf ../intercept/ ./build/",
    "startforever": "./scripts/startForever.sh",
    "stopforever": "./scripts/stopForever.sh",
    "test": "cd ./client && npm run test",
    "help": "if ./scripts/noDir.sh ./build; then npm run build; fi; node ./build/app.js --help; exit 0",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allproxy/allproxy.git"
  },
  "author": "Dave Christenson",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "axios": "^0.21.4",
    "bufferutil": "^4.0.3",
    "decompress-response": "^3.3.0",
    "electron-find": "^1.0.7",
    "electron-squirrel-startup": "^1.0.0",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "net": "^1.0.2",
    "node-fetch": "^2.6.7",
    "path": "^0.12.7",
    "protobufjs": "^6.11.2",
    "socket.io": "^4.0.1",
    "sql-formatter": "^2.3.3",
    "tls": "^0.0.1",
    "url": "^0.11.0",
    "utf-8-validate": "^5.0.4",
    "yarpm": "^1.1.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-dmg": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@types/jquery": "^3.5.5",
    "@types/node": "^14.14.35",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "@types/sql-formatter": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "electron": "^19.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "icons/icon",
        "ignore": [
          "^.*.(pem,key,settings,vscode,*ignore,project,yml,ts,tsx,json)$"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "allproxy"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
